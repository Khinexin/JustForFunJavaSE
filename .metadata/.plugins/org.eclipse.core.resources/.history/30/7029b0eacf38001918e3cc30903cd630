

package demo;

import java.util.Scanner;

public class PyrimidPatternNumber {

	public static void main(String[] args) {
		
		System.out.println("Enter a num to reverse pyramid!");
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		
		for(int i=num; i>=1;i--) {
			for(int j=1; j<=i ; j++) {
				System.out.print(j);
			}
			System.out.println();
		}
	}

}
