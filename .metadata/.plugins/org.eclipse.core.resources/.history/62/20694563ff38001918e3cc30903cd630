package demo;

import java.util.Arrays;
import java.util.Collection;
import java.util.Comparator;
import java.util.List;

public class BubbleSort {
	public static void main(String[] args) {	
		
		List<Integer> numberList = Arrays.asList(4,5,6,2,1,3,10);
		
		System.out.println(numberList);
		
//		List<Integer> list = numberList.stream()
//				.sorted((a,b)-> a.compareTo(b)).forEach(System.out::println);
		
		List<Integer> newList = numberList.stream().sorted((a,b) -> a.compareTo(b)).forEach(System.out::println));		
		
	}
}
